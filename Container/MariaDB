docker run --privileged -d --name=mariadb \
 --net mariadb \
 --dns 10.0.5.3 \
 --restart always \
 -v /etc/localtime:/etc/localtime:ro \
 -v /mnt/data/proxymanager/mysql:/var/lib/mysql \
 -e MYSQL_ROOT_PASSWORD=npm" \
 -e MYSQL_DATABASE=npm" \
 -e MYSQL_USER=npm" \
 -e MYSQL_PASSWORD=npm" \
 jc21/mariadb-aria:latest
